<head>
    <meta charset="utf-8">
    <title>CHASHMA Dashboard</title>
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta property="og:title" content="">
    <meta property="og:type" content="">
    <meta property="og:url" content="">
    <meta property="og:image" content="">
    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="/assetsb/imgs/theme/toplogo.jpeg">
    <!-- Template CSS -->
    <link href="/assetsb/css/main.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <div class="screen-overlay"></div>
    <%-include('aside')-%>
    <main class="main-wrap">
       <%-include('header')-%>
        <section class="content-main">
            <div class="content-header">
                <h2 class="content-title">Users list</h2>
                
            </div>
            <div class="card mb-4">
               
                <div class="card-body">
                    <div class="table-responsive">
                        <table class="table table-hover" id="table">
                            <thead>
                                <tr>
                                    <th>Seller</th>
                                    <th>Email</th>
                                    <th>Status</th>
                                    <th class="text-end"> Action </th>
                                </tr>
                            </thead>
                           
                            <tbody>
                                <% if(users.length> 0){%>

                                    <% for(let i=0;i<users.length;i++){ %>
                                <tr>
                                    <td width="40%">
                                        <a href="#" class="itemside">
                                            <div class="left">
                                                <img src="https://t4.ftcdn.net/jpg/03/31/69/91/360_F_331699188_lRpvqxO5QRtwOM05gR50ImaaJgBx68vi.jpg" class="img-sm img-avatar" alt="Userpic">
                                            </div>
                                            <div class="info pl-3">
                                                <h6 class="mb-0 title"> <%= users[i].name %></h6>
                                                <small class="text-muted"> <%= users[i].mobile %></small>
                                            </div>
                                        </a>
                                    </td>
                                    <td>  <%= users[i].email %></td>
                                    
                                    <td>
                                               
                                        <% if(users[i].is_active) { %>
                            <span class="badge rounded-pill alert-success">Active</span>
                                        <% } else { %>
                            <span class="badge rounded-pill alert-danger">Inactive</span>
                                        <% } %>
                                    
                                </td>
                                
                                <td class="text-end">
                                    <%if(users[i].is_active){%>
                                        
                                        <button onclick="sweet('<%= users[i]._id %>')" value="0" class="btn btn-sm btn-danger rounded font-sm mt-15">Block</button>
                                    
                                        <%}else{%>
                                            <button onclick="sweetalert('<%= users[i]._id %>')" value="1" class="btn btn-sm btn-success rounded font-sm mt-15">UnBlock</button>
                                        <%}%>

                                   
                                
                                </td>
                                 
                                </tr>
                                <%}}%>
                            </tbody>
                           
                        </table> <!-- table-responsive.// -->
                        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
                            <script src="https://cdn.datatables.net/1.13.6/js/jquery.dataTables.min.js"></script>
                            <script src="https://cdn.datatables.net/1.13.6/js/dataTables.bootstrap5.min.js"></script>
                            <script>
                                $(document).ready(function () {
                                    new DataTable('#table');
                            
                            })
                            </script>
                    </div>
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
           
        </section> <!-- content-main end// -->
        <%-include('footer')-%>
    </main>
    <script>
        function sweet(userId){
            Swal.fire({
  title: "Are you sure?",
  text: "You want to block User!",
  icon: "warning",
  showCancelButton: true,
  confirmButtonColor: "#3085d6",
  cancelButtonColor: "#d33",
  confirmButtonText: "Yes, Block User!"
}).then((result) => {
  if (result.isConfirmed) {
    Swal.fire({
      title: "Blocked!",
      text: "User has been blocked.",
      icon: "success"
    }).then((res)=>{
        if(res.isConfirmed){
            window.location.href=`useraction?userId=${userId}&act=0`
        }
    })
  }
});
        }

        function sweetalert(userId) {
    Swal.fire({
        title: "Are you sure?",
        text: "You want to unblock user!",
        icon: "warning",
        showCancelButton: true,
        confirmButtonColor: "#3085d6",
        cancelButtonColor: "#d33",
        confirmButtonText: "Yes, Unblock User!",
    }).then((result) => {
        if (result.isConfirmed) {
            Swal.fire({
                title: "Unblocked!",
                text: "User has been Unblocked.",
                icon: "success",
            }).then((res) => {
                if (res.isConfirmed) {
                    window.location.href = `useraction?userId=${userId}&act=1`;
                }
            });
        }
    });
}

    </script>


    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="/assetsb/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="/assetsb/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="/assetsb/js/vendors/select2.min.js"></script>
    <script src="/assetsb/js/vendors/perfect-scrollbar.js"></script>
    <script src="/assetsb/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="/assetsb/js/main.js" type="text/javascript"></script>
</body>


